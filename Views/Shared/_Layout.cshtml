        var buildMessage = function (option) {
            option = option || { message: "Please provide message!", className: "success" }
            var $messageDiv = $("<div>").addClass(option.className).html(option.message);
            return $messageDiv;
        };

        $(document).ready(function () {




            $("nav li").click(function (e) {
                $("nav li ").removeClass("active"); //Remove any "active" class  
                $(this).addClass("active"); //Add "active" class to selected tab  
            });

            $('[data-toggle="customDropdown"]').on('click', function () {
                $(this).parent().toggleClass('open');
            });
            $('[data-toggle="subdropdown"]').on('click', function () {
                $(this).parent().toggleClass('open');
                $('[data-toggle="subdropdown1"]').parent().removeClass('open');
            });
            $('[data-toggle="subdropdown1"]').on('click', function () {
                $(this).parent().toggleClass('open');
                $('[data-toggle="subdropdown"]').parent().removeClass('open');
            });
            $('[data-toggle="dropdown"]').on('click', function () {
                //$('[data-toggle="subdropdown"]').parent().removeClass('open');
                //$('[data-toggle="subdropdown1"]').parent().removeClass('open');
                $('[data-toggle="customDropdown"]').parent().removeClass('open');
                $('[data-toggle="subdropdown"]').parent().removeClass('open');
                $('[data-toggle="subdropdown1"]').parent().removeClass('open');
            });


            $('body').bind('click', function (e) {
                if ($(e.target).closest('.open').length == 0 && $(e.target).closest('.dropdown').length == 0) {
                    // click happened outside of .navbar, so hide
                    var opened = $('.dropdown').hasClass('open');
                    if (opened === true) {
                        $('.dropdown').removeClass('open');
                        $('.submenu').removeClass('open');
                    }
                }
            });


        });

        var isLoggedIn = function () {
            var option = {
                url: "/Account/IsLoggedIn",
                method: "GET"
                ,async: 'false'
            };

            $.ajax(option).done(function (response) {
                if (response.isLogged == false) {
                    window.location = response.rededirection_url;
                }
            });
        };

        var isNonLoggedIn = function () {
            var option = {
                url: "/Account/IsNonLoggedIn",
                method: "GET"
                ,async:'false'
            };

            $.ajax(option).done(function (response) {
                if (response.isLogged == false) {
                    window.location = response.rededirection_url;
                }
            });
        };

        $(".alert-success").fadeOut(5000);
        $(".alert-danger").fadeOut(5000);
