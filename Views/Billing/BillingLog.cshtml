            <div class="col-sm-6 col-md-4">
                @Html.TextBoxFor(model => model.OfficeName, new { @id = "txtBOfficeName", @maxlength = "100", @class = "form-control" })
                @Html.ValidationMessageFor(model => model.OfficeName)
