                    <script type="text/javascript">
                        var limit = 200;
                        function check() {

                        }
                        if (document.BillingLog.AdditionalComments.value.length > limit) {
                            alert('Too much data!');
                            document.BillingLog.AdditionalComments.focus();
                            //return false;
                        }
                        else
                            //return true;
                            //}
                            function update() {
                                var old = document.BillingLog.counter.value;
                                document.BillingLog.counter.value = document.BillingLog.AdditionalComments.value.length;
                                if (document.BillingLog.counter.value > limit && old <= limit) {
                                    alert('Too much data in the text box! Maximum number of characters is 200');
                                    if (document.styleSheets) {
                                        document.BillingLog.counter.style.fontWeight = 'bold';
                                        document.BillingLog.counter.style.color = '#ff0000';
                                    }
                                }
                                else if (document.BillingLog.counter.value <= limit && old > limit
                                    && document.styleSheets) {
                                    document.BillingLog.counter.style.fontWeight = 'normal';
                                    document.BillingLog.counter.style.color = '#000000';
                                }
                            }
                    </script>
                    <script type="text/javascript">
                        document.write('Characters typed: <input ' + 'type="text" size="3" name="counter" value=""' + 'readonly onfocus="this.form.AdditionalComments.focus()" class="lmtchr"> (limit: ' + limit + ')');
