@model Mbpros.Models.BillingModel
@{
    ViewBag.Title = "BillingLog";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="form_desc">
    @if (Model.BillingID != 0)
    {
        <h1>Edit Billing Log Form</h1>
    }
    else
    {
        <h1>Billing Log Form</h1>
    }
    <div class="head_subtitle">Complete the below secure form to provide billing information to MBP.</div>
    <ul class="form_rule">
        <li>Do NOT use special characters like & or ' or :.</li>
        <li>For providers using MBP's complete Accounts Receivable Posting Service, report patient co-payments and deductibles paid at the time of visit by entering the dollar amount in - parentheses next to the date of service.</li>
        <li>To see an example of how to report patient co-payments paid, <a href="https://www.mbpros.com/billinglogform.png">click here</a></li>
        <li>To report all other patient payments received, use our form at <a href="http://www.mbpros.com/patient_payments.html">http://www.mbpros.com/patient_payments.htm </a></li>
    </ul>
    <p>(<i class="red">*</i> Indicates required field)</p>
</div>


<div class="form_border">

    @using (Html.BeginForm("BillingLog", "Billing", FormMethod.Post, new { enctype = "multipart/form-data", name = "BillingLog", id = "BillingLog", @class = "form-horizontal customIzeFormELement normal_label" }))
    {
        if (ViewBag.ReCaptchaMessage != "" && ViewBag.ReCaptchaMessage != null)
        {
            <div class="alert alert-danger">
                <strong>Warning! </strong>@ViewBag.ReCaptchaMessage.
            </div>
        }
        @* <form class="form-horizontal customIzeFormELement normal_label" name = "BillingLog" id = "BillingLog">*@
        @Html.ValidationSummary(true)
        @Html.HiddenFor(model => model.BillingID)
        @Html.HiddenFor(model => model.CallFrom, new { @id = "hdnCallFrom" })

        @Html.Hidden("CaptchaSitekeychallenge")
        @Html.Hidden("CaptchaSitekeynoscript")
        @Html.Hidden("CaptchaSecretkey")

        if (Model.BillingID != 0)
        {
            <div class="form-group">
                <label for="inputEmail3" class="col-sm-3 col-md-2 control-label">Billing Form Id:</label>
                <div class="col-sm-6 col-md-4">
                    @Html.TextBoxFor(model => model.BillingID, new { @maxlength = "100", @class = "form-control", @readonly = "readonly" })
                </div>
            </div>
            <div class="form-group">
                <label for="inputEmail3" class="col-sm-3 col-md-2 control-label">Submit Date:</label>
                <div class="col-sm-6 col-md-4">
                    @Html.TextBoxFor(model => model.CreatedDate, new { @maxlength = "100", @class = "form-control", @readonly = "readonly" })
                </div>
            </div>
        }
        <div class="form-group  required">
